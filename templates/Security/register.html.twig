{% extends "base.html.twig" %}

{% block title %}Register!{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-sm-12">
            <h1>{{ page }}</h1>
            <div class="well">
                {% if error %}
                    <div class="alert alert-danger">
                        {{ error.messageKey | trans(error.messagedata, 'security') }}
                    </div>
                {% endif %}
                {{ form_start(form) }}
                    {{ form_errors(form) }}
                    {{ form_rest(form) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}
